# java home
JAVA_HOME=/usr/lib/jvm/jre-1.7.0-oracle.x86_64/

# xmlValidator property file
CONFIGURATION_xmlValidator=./serviceServer/TestXmlValidatorRequestHandler.props


# add java;openJms to PATH 
PATH=${JAVA_HOME}/bin:${OPENJMS}:${PATH}
echo "new PATH=${PATH}"

export JAVA_HOME
export PATH
export OPENJMS

